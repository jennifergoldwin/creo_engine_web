        <div class="container">
            <div class="row " style="margin-top: 7%;" >
                <div class="col-md-5">
                    <span class="team-text" id="our-team-text">
                        Our Team
                    </span>
                    <p class="team-text mobile-text" >The CreoEngine team is a group of developers, operators, marketers,
                        and designers with expertise in their respective fields
                    </p>
            
                    <p class="team-text desktop-text">The CreoEngine team is a group of developers, 
                        <br>operators, marketers, and designers with exper-<br>tise in their respective fields
                    </p>

                </div>
                <div class="col-md-7 align-items-center" >
                    <!-- <div class="team-container">
                        <img class="img-box" src="public/assets/team.png"> 
                    </div> -->
                    <table class="desktop-container">
                        <tr>
                            <% for (var i = 0; i < 5; i++) { %>
                                  <td>
                                      <div style="width: 140px; height: 140px; background-size: cover;">
                                          <img src="<%= list[i] %>" style="max-width: 100%; max-height: 100%;">
                                      </div>
                                  </td>
                                   
                            <% } %>
                        </tr>
                        <tr>
                            <% for (var i = 5; i < 9; i++) { %>
                                <td>
                                    <div style="width: 150px; height: 150px; background-size: cover;">
                                        <img src="<%= list[i] %>" style="max-width: 100%; max-height: 100%;">
                                    </div>
                                </td>
                                       
                            <% } %>
                        </tr> 
                        <tr>
                            <% for (var i = 9; i < 13; i++) { %>
                                <td>
                                    <div style="width: 150px; height: 150px; background-size: cover;">
                                        <img src="<%= list[i] %>" style="max-width: 100%; max-height: 100%;">
                                    </div>
                                </td>
                                       
                            <% } %>
                        </tr> 
                    </table>

                    <table class="mobile-container">
                        <tr>
                            <% for (var i = 0; i < 3; i++) { %>
                                  <td>
                                      <div style="height: 130px; width: 100px;">
                                          <img src="<%= list[i] %>" style="max-width: 100%; max-height: 100%;">
                                      </div>
                                  </td>
                                   
                            <% } %>
                        </tr>
                        <tr>
                            <% for (var i = 3; i < 6; i++) { %>
                                  <td>
